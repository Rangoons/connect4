<div class="container gbc">
  <div class="row">
    <div class="col-lg-1">
      <div class="p1-score">
        <p>0</p>
      </div>
    </div>
    <div class="col-lg-6" id="board">
      <table class="center-table">
      </table>
      <div class="form-group">
        <label class="col-sm-3 control-label share-label" for="shareUrl">Share URL: {{{shareURL}}}</label>
        <div class="col-sm-9">
          <input class="form-control" type="text" ReadOnly="ReadOnly"/>
        </div>
      </div>
    </div>
    <div class="col-lg-1">
      <div class="p2-score">
        <p>0</p>
      </div>
    </div>
    <div class="col-lg-4">
          {{>chat}}
    </div>

  </div>
</div>
<script src="/socket.io/socket.io.js"></script>
<script type="text/javascript">
$(document).ready(function() {
    for(var i = 0; i < 6; i++){
        $('#board table').append('<tr></tr>');
        for(var j = 0; j < 7; j++) {
            $('#board tr').last().append('<td></td>');
            $('#board td').last().addClass('box').attr('data-row', i).attr('data-column', j);
        }
    }
});
</script>
<script src="\js\gamelogic.js" charset="utf-8"></script>
